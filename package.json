{
  "name": "lunchfinder.io",
  "version": "1.0.0",
  "author": "Maxime Dehaye <maxime.dehaye@gmail.com>",
  "homepage": "https://lunchfinder.io",
  "private": true,
  "now": {
    "alias": "lunchfinder.io"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && cd build && now && cd ../ && now alias",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "coverage": "react-scripts test --env=jsdom --coverage --setupTestFrameworkScriptFile=raf/polyfill",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "google-map-react": "0.24.0",
    "husky": "0.14.3",
    "lint-staged": "4.2.3",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "7.0.0",
    "npm-run-all": "4.0.2",
    "prettier": "1.7.4",
    "prop-types": "15.6.0",
    "query-string": "5.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-places-autocomplete": "5.4.2",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.14"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.0.0",
    "reflux": "6.4.1",
    "sinon": "4.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --print-width 160",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/styles/*.{js,jsx}",
      "!src/index.js",
      "!src/ServiceWorker.js"
    ]
  }
}
