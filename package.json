{
  "name": "lunchfinder.io",
  "version": "1.0.0",
  "author": "Maxime Dehaye <maxime.dehaye@gmail.com>",
  "homepage": "https://lunchfinder.io",
  "private": true,
  "now": {
    "alias": "lunchfinder.io"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "yarn run build && now ./build && now alias --alias lunchfinder.io",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "google-map-react": "0.25.0",
    "history": "4.7.2",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "7.0.0",
    "now": "10.0.0-canary.10",
    "npm-run-all": "4.1.2",
    "prettier": "1.8.2",
    "prop-types": "15.6.0",
    "query-string": "5.0.1",
    "raf": "3.4.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-places-autocomplete": "5.4.3",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17",
    "react-test-renderer": "16.1.1",
    "reflux": "6.4.1",
    "sinon": "4.1.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --print-width 160",
      "git add"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/styles/*.{js,jsx}",
      "!src/images/icons/*.{js,jsx}",
      "!src/index.js",
      "!src/Store.js",
      "!src/ServiceWorker.js"
    ]
  }
}
